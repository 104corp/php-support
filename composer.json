{
    "name": "104corp/support",
    "description": "The 104corp support package.",
    "type": "library",
    "license": "BSD-3-Clause",
    "require": {
        "psr/container": "^1.0",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "^6.3",
        "phpunit/phpunit": "<6",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Corp104\\Support\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Corp104\\Support\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    }
}
