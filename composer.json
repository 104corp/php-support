{
    "name": "104corp/support",
    "description": "The 104corp support package.",
    "type": "library",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/104corp/php-support/issues"
    },
    "require": {
        "php": "^7.4 | ^8.0",
        "psr/container": "^1 | ^2",
        "psr/log": "^1 | ^2 | ^3"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "^6.3 | ^7",
        "phpunit/phpunit": "^9",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "Corp104\\Support\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Corp104\\Support\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    }
}
